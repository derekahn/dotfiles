{
  "let mut": {
    "prefix": "letm",
    "body": ["let mut ${1:pat} = ${2:expr};"],
    "description": "let mut … = …;"
  },
  "println": {
    "prefix": "pn",
    "body": ["println!(\"${1}\");"],
    "description": "println!(…);"
  },
  "method": {
    "prefix": "meth",
    "body": [
      "fn ${1:name}(${2:&self}) -> ${3:RetType} {",
      "    ${4:unimplemented!();}",
      "}"
    ],
    "description": "method"
  },
  "public method": {
    "prefix": "pmeth",
    "body": [
      "pub fn ${1:name}(${2:&self}) -> ${3:RetType} {",
      "    ${4:unimplemented!();}",
      "}"
    ],
    "description": "method"
  },
  "String::from": {
    "prefix": "st",
    "body": ["String::from(\"${1}\")${2}"],
    "description": "String::from()"
  },
  "<T>": {
    "prefix": "tt",
    "body": ["<${1:T}>"],
    "description": "Generic instantion"
  },
  "trait method": {
    "prefix": "sig",
    "body": ["fn ${1:name}(${2:&self}) -> ${3:RetType}; "],
    "description": "Method signature"
  },
  "cfg": {
    "prefix": "cfgt",
    "body": ["#[cfg(${1:test})]"],
    "description": "cfg attribute"
  },
  "ignore": {
    "prefix": "ignore",
    "body": ["#[ignore]"],
    "description": "ignore attribute"
  }
}
